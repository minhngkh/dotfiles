    "editor.background": "#1e1e1e",
    "editor.gutter.background": "#1e1e1e",
    "tab_bar.background": "#1e1e1e",
    "terminal.background": "#1e1e1e",
    "toolbar.background": "#1e1e1e",
    "status_bar.background": "#1e1e1e",
    "title_bar.background": "#1e1e1e",
