{
  "assistant": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "claude-3-5-sonnet"
    },
    "version": "2"
  },
  "base_keymap": "VSCode",
  "theme": "monokai (Filter Spectrum)",
  // "ui_font_size": 16,
  // "buffer_font_size": 14.6,
  // "tab_size": 2,
  // "buffer_font_family": "JetGraph Nerd Font",
  // "buffer_font_family": "JetBrainsMono Nerd Font",
  "buffer_font_family": "Source Code Pro",
  // "buffer_font_fallbacks": ["JetBrainsMono Nerd Font"],
